{
  "name": "express-package-json",
  "version": "1.0.2",
  "description": "express middleware to expose the contents of package.json to the view engine via res.locals",
  "main": "index.js",
  "scripts": {
    "pretest": "jshint index.js",
    "test": "mocha -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tcort/express-package-json"
  },
  "keywords": [
    "express",
    "package.json",
    "package",
    "json",
    "template"
  ],
  "author": "Thomas Cort <linuxgeek@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tcort/express-package-json/issues"
  },
  "homepage": "https://github.com/tcort/express-package-json",
  "devDependencies": {
    "mocha": "^8.4.0",
    "supertest": "^6.1.3",
    "express": "^4.17.1",
    "hbs": "^4.1.2",
    "jshint": "^2.12.0"
  },
  "jshintConfig": {
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "globalstrict": true,
    "immed": true,
    "indent": 4,
    "moz": true,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "trailing": true,
    "undef": true,
    "smarttabs": true,
    "strict": true,
    "validthis": true,
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "beforeEach": false,
      "after": false,
      "afterEach": false
    }
  }
